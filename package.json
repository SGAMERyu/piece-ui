{
  "name": "piece-ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "commit": "npx git-cz",
    "build": "turbo run build",
    "docs": "pnpm -C docs run docs",
    "dev": "turbo run dev --parallel",
    "play": "pnpm -C packages/piece-ui run play",
    "build:icon": "pnpm -C packages/piece-icon run build",
    "build:component": "pnpm -C packages/piece-ui run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@piece-ui/icon": "workspace:*",
    "piece-ui": "workspace:*"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@storybook/addon-a11y": "^6.5.10",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@unocss/core": "^0.45.13",
    "@unocss/transformer-directives": "^0.45.21",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-vue": "^9.3.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "latest",
    "rimraf": "^3.0.2",
    "scule": "^0.3.2",
    "tsup": "^6.2.3",
    "turbo": "latest",
    "typescript": "^4.7.4",
    "unocss": "^0.45.21",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-define-options": "^0.11.1",
    "vite": "^3.0.7",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-node-polyfills": "^0.1.8",
    "vitepress": "1.0.0-alpha.15"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.1"
}
